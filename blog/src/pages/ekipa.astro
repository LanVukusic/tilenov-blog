---
import Outline from "../layouts/Outline.astro";
---

<Outline title="Ekipa">
  <div class="bg-white rounded-xl z-20 p-8 shadow-xl my-16 mx-auto">
    <div class="flex py-4">
      <div class="w-16 h-16 flex" style="transform: rotate(-20deg); ">
        <img src="listek1.png" />
      </div>
      <h1 class="text-6xl manslava mb-8">Ekipca</h1>
    </div>
    <p class="vollkorn text-xl py-4 text-justify"></p>

    <div class="text-right w-full reenieBeenie text-4xl pt-8">Tilen Letner</div>
  </div>
</Outline>

<style>
  .manslava {
    font-family: "Mansalva", cursive;
  }
  .vollkorn {
    font-family: "Vollkorn", serif;
  }
  .reenieBeenie {
    font-family: "Reenie Beanie", cursive;
  }
</style>
